file = {:home/raul/temp/qgasqtheo/QuantumComputingTheory-Literature/VQE/Barren_Plateau/Barren_Plateaus_In_Quantum_Neural_Network_McClean_2018.pdf:pdf},
issn = {20411723},
journal = {Nature Communications},
number = {1},
pages = {1--6},
pmid = {30446662},
publisher = {Springer US},
title = {{Barren plateaus in quantum neural network training landscapes}},
url = {http://dx.doi.org/10.1038/s41467-018-07090-4},
volume = {9},
year = {2018}
}

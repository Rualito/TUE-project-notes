file = {:home/raul/temp/qgasqtheo/QuantumComputingTheory-Literature/Rydberg_Quantum_Computing_and_Simulation/Analog_Quantum_Chemistry_Simulation_Arguello_2018.pdf:pdf},
issn = {14764687},
journal = {Nature},
number = {7777},
pages = {215--218},
pmid = {31597975},
title = {{Analogue quantum chemistry simulation}},
volume = {574},
year = {2019}
}

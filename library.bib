file = {:home/raul/temp/qgasqtheo/QuantumComputingTheory-Literature/VQE/Barren_Plateau/Cost_Function_Dependent_Barren_Plateaus_in_Shallow_Parametrized_Quantum_Circuits_Cerezo_2021.pdf:pdf},

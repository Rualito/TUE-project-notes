file = {:home/raul/temp/qgasqtheo/QuantumComputingTheory-Literature/Rydberg_Quantum_Computing_and_Simulation/Simulating_Polaron_Biophysics_with_Rydberg_Plodzien_2017.pdf:pdf},
issn = {20452322},
journal = {Scientific Reports},
number = {1},
pages = {1--7},

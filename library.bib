arxivId = {2008.01098},
author = {Choquette, Alexandre and {Di Paolo}, Agustin and Barkoutsos, Panagiotis Kl and S{\'{e}}n{\'{e}}chal, David and Tavernelli, Ivano and Blais, Alexandre},
